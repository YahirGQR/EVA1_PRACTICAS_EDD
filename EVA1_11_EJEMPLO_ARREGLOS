public class EVA1_11_EJEMPLO_ARREGLOS {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        
        Scanner teclado = new Scanner(System.in);
        System.out.println("Ingrese numero de grupos: 2");
        int g = teclado.nextInt();
         
        // JOptionPane.showInputDialog(null, "Ingrese numero de grupos: ");
        
        int[][] calificaciones = new int[g][];
        
        for (int i = 0; i < calificaciones[g].length; i++) {
            System.out.println(""); 
            
        }
        /*calificaciones[] = new int[];
        System.out.println("Ingrese numero de grupos:" + g); 
        */
        
        // para cada grupo cuantos estudiantes van a capturar 
        
    }
    
